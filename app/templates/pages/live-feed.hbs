<section class="toys-book-form" id="live-feed-book-form" data-toggle-form="bookFormStatus.visible">
	<div class="page-section-header">
		<h2 class="page-section-title">
			<span class="br">WANT THE LIVE FEED </span>
			<span class="br">AT YOUR NEXT EVENT?</span>
		</h2>

		<div class="bookForm-control">
			<button class="bookForm-toggle" data-ng-click="bookFormStatus.visible = true" data-ng-hide="bookFormStatus.visible">Hire Now</button>
			<button class="bookForm-close" data-ng-click="bookFormStatus.visible = false" data-ng-show="bookFormStatus.visible">
				<span class="visuallyhidden">Close</span>
				<span class="icon-close"></span>
			</button>
		</div>
	</div>

	<div class="bookForm-wrapper" data-ng-class="{visible: bookFormStatus.visible}">
		<form class="bookForm" name="bookForm" novalidate>
			<div class="input-row input-row-2cols">
				<div class="input-col">
					<label for="name">Name</label>
					<input type="text" name="name" id="name" placeholder="name" data-placeholder-fallback="name" data-ng-model="bookFields.name" required>
				</div><div class="input-col">
					<label for="phone">Phone</label>
					<input type="text" name="phone" id="phone" placeholder="phone" data-placeholder-fallback="Phone" data-ng-model="bookFields.phone" required>
				</div>
			</div>

			<div class="input-row">
				<div class="input-col">
					<label for="cemail">Email</label>
					<input type="email" name="cemail" id="cemail" placeholder="email" data-placeholder-fallback="email" data-ng-model="bookFields.email" required>
				</div>
			</div>

			<div class="input-row">
				<div class="input-col">
					<label for="message">Message</label>
					<textarea name="message" id="message" placeholder="message" data-placeholder-fallback="message" data-ng-model="bookFields.message" required></textarea>
				</div>
			</div>

			<div class="input-row">
				<div class="input-col">
					<button type="submit" class="submit-bookForm" data-ng-click="book(bookFields)" data-ng-disabled="bookForm.submitting">
						<span data-ng-hide="bookForm.submitting">Submit</span>
						<span data-ng-hide="!bookForm.submitting">Submitting</span>
					</button>
				</div>
			</div>

			<div class="form-error error-bookForm" data-ng-class="{'has-error': bookForm.$invalid && bookForm.clicked}">
				<div class="error-message" data-ng-show="bookForm.name.$error.required">Name is required</div>
				<div class="error-message" data-ng-show="bookForm.cemail.$error.required">Email is required</div>
				<div class="error-message" data-ng-show="bookForm.cemail.$error.email">Email is invalid</div>
				<div class="error-message" data-ng-show="bookForm.phone.$error.required">Phone is required</div>
				<div class="error-message" data-ng-show="bookForm.message.$error.required">Message is required</div>
			</div>

			<div class="form-response response-bookForm" data-ng-class="{'has-response': bookForm.response}" data-ng-hide="bookForm.clicked">
				{%bookForm.response%}
			</div>
		</form>
	</div>
</section>

<section class="toys-cta" id="live-feed-cta">
	<div class="page-section-header">
		<h2 class="page-section-title">
			<span class="br">LIKE THE LIVE FEED? </span>
			<span class="br">WHY NOT CHECK OUT OUR </span>
			<span class="br">INSTAGRAM PRINTER?</span>
		</h2>
		<a href="/instagram-printer/" class="toys-cta-link" id="live-feed-cta-link">INSTAGRAM PRINTER</a>
	</div>
</section>