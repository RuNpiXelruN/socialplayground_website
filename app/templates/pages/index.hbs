<section id="partners" class="page-section" data-section data-toggle-form="contactFormStatus.visible">
	<header class="page-section-header">
		<h2 class="page-section-title">
			PARTNERS.
		</h2>
		<p class="page-section-subtitle">
			<span class="br">Interested in a commercial license? We’re on the lookout </span>
			<span class="br">for global playmates. Get in touch to find out more.</span>
		</p>

		<div id="contactForm-control">
			<button id="contactForm-toggle" data-ng-click="contactFormStatus.visible = true" data-ng-hide="contactFormStatus.visible">Enquire</button>
			<button id="contactForm-close" data-ng-click="contactFormStatus.visible = false" data-ng-show="contactFormStatus.visible">
				<span class="visuallyhidden">Close</span>
				<span class="icon-close"></span>
			</button>
		</div>
	</header>

	<div id="contactForm-wrapper" data-ng-class="{visible: contactFormStatus.visible}">
		<form id="contactForm" name="contactForm" novalidate>
			<div class="input-row input-row-2cols">
				<div class="input-col">
					<label for="name">Name</label>
					<input type="text" name="name" id="name" placeholder="name" data-placeholder-fallback="name" data-ng-model="contactFields.name" required>
				</div><div class="input-col">
					<label for="phone">Phone</label>
					<input type="text" name="phone" id="phone" placeholder="phone" data-placeholder-fallback="Phone" data-ng-model="contactFields.phone" required>
				</div>
			</div>

			<div class="input-row">
				<div class="input-col">
					<label for="cemail">Email</label>
					<input type="email" name="cemail" id="cemail" placeholder="email" data-placeholder-fallback="email" data-ng-model="contactFields.email" required>
				</div>
			</div>

			<div class="input-row">
				<div class="input-col">
					<label for="message">Message</label>
					<textarea name="message" id="message" placeholder="message" data-placeholder-fallback="message" data-ng-model="contactFields.message" required></textarea>
				</div>
			</div>

			<div class="input-row">
				<div class="input-col">
					<button type="submit" id="submit-contactForm" data-ng-click="contact(contactFields)" data-ng-disabled="contactForm.submitting">
						<span data-ng-hide="contactForm.submitting">Submit</span>
						<span data-ng-hide="!contactForm.submitting">Submitting</span>
					</button>
				</div>
			</div>

			<div class="form-error" id="error-contactForm" data-ng-class="{'has-error': contactForm.$invalid && contactForm.clicked}">
				<div class="error-message" data-ng-show="contactForm.name.$error.required">Name is required</div>
				<div class="error-message" data-ng-show="contactForm.cemail.$error.required">Email is required</div>
				<div class="error-message" data-ng-show="contactForm.cemail.$error.email">Email is invalid</div>
				<div class="error-message" data-ng-show="contactForm.phone.$error.required">Phone is required</div>
				<div class="error-message" data-ng-show="contactForm.message.$error.required">Message is required</div>
			</div>

			<div class="form-response" id="response-contactForm" data-ng-class="{'has-response': contactForm.response}" data-ng-hide="contactForm.clicked">
				{%contactForm.response%}
			</div>
		</form>
	</div>
</section>