<section id="newsletter" class="page-section">
	<header class="page-section-header">
		<h3 class="page-section-subtitle">
			<span class="br">Join our mailing list and we’ll keep you up to date </span>
			<span class="br">on all the fun and games.</span>
		</h3>
	</header>
	<form id="signupForm" name="signupForm" novalidate>
		<div id="inputWrapper-signupForm">
			<label for="email-signupForm" class="visuallyhidden">Email</label>

			<input type="email" name="email" id="email-signupForm" placeholder="Enter your email" data-placeholder-fallback="Enter your email" required data-ng-model="signupFields.email" data-ng-class="{submitting: signupForm.submitting}">

			<button type="submit" id="submit-signupForm" data-ng-click="signup(signupFields)" data-ng-disabled="signupForm.submitting">
				<span data-ng-hide="signupForm.submitting">Join</span>
				<span data-ng-hide="!signupForm.submitting">Submitting</span>
			</button>
		</div>

		<div class="form-error" id="error-signupForm" data-ng-class="{'has-error': signupForm.$invalid && signupForm.clicked}">
			<span data-ng-show="signupForm.email.$error.required">Email is required</span>
			<span data-ng-show="signupForm.email.$error.email">Email is invalid</span>
			<span data-ng-show="signupForm.errorEmail">{%signupForm.errorEmail%}</span>
		</div>

		<div class="form-response" id="response-signupForm" data-ng-class="{'has-response': signupForm.response}" data-ng-hide="signupForm.clicked">
			{%signupForm.response%}
		</div>
	</form>
</section>

